<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Control.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tender</servlet-name>
        <servlet-class>Control.tender</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Control.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tendersApplied</servlet-name>
        <servlet-class>Control.tendersApplied</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tendersWon</servlet-name>
        <servlet-class>Control.tendersWon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tendersRejected</servlet-name>
        <servlet-class>Control.tendersRejected</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateTender</servlet-name>
        <servlet-class>Control.CreateTender</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TenderApplicants</servlet-name>
        <servlet-class>Control.TenderApplicants</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApproveTender</servlet-name>
        <servlet-class>Control.ApproveTender</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tenderApplication</servlet-name>
        <servlet-class>Control.tenderApplication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Apply</servlet-name>
        <servlet-class>Control.Apply</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>Control.Registration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tender</servlet-name>
        <url-pattern>/tender</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tendersApplied</servlet-name>
        <url-pattern>/tendersApplied</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tendersWon</servlet-name>
        <url-pattern>/tendersWon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tendersRejected</servlet-name>
        <url-pattern>/tendersRejected</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateTender</servlet-name>
        <url-pattern>/CreateTender</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TenderApplicants</servlet-name>
        <url-pattern>/TenderApplicants</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApproveTender</servlet-name>
        <url-pattern>/ApproveTender</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tenderApplication</servlet-name>
        <url-pattern>/tenderApplication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Apply</servlet-name>
        <url-pattern>/Apply</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
